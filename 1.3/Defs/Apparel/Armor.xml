<Defs>
  <!-- SpecOps armor -->
  <ThingDef ParentName="ArmorMachineableBase">
    <defName>AvaliSpecOpsArmor</defName>
    <thingCategories>
      <li>AvaliApparel</li>
      <li>Apparel</li>
    </thingCategories>
    <label>Avali spec ops armor</label>
    <description>A set of dark Avali special operations armour. Covering nearly their entire body barring their head, this large suit of armour provides the wearer with much protection from any hostile elements. Be it temperature, bullets, physical attacks or even toxic particles. Regretfully, due to its large size and somewhat bulky nature—the wearer has a slightly harder time moving but that’s the cost for survival.</description>
    <techLevel>Spacer</techLevel>
    <recipeMaker>
      <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
      <researchPrerequisite>AvaliSpecOpsArmor</researchPrerequisite>
      <skillRequirements>
        <Crafting>15</Crafting>
      </skillRequirements>
      <recipeUsers Inherit="False">
        <li>AvaliNanoForge</li>
      </recipeUsers>
    </recipeMaker>
    <graphicData>
		<texPath>RimValiFFA/Things/Apparel/SpecOpsArmor/SpecOpsArmor_Avali_south</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <uiIconScale>1.25</uiIconScale>
    <tradeTags>
      <li>HiTechArmor</li>
    </tradeTags>
    <generateCommonality>2</generateCommonality>
    <statBases>
      <WorkToMake>50000</WorkToMake>
      <MaxHitPoints>400</MaxHitPoints>
      <Mass>5</Mass>
      <Flammability>0.4</Flammability>
      <EquipDelay>10</EquipDelay>
      <ArmorRating_Sharp>0.98</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.43</ArmorRating_Blunt>
      <ArmorRating_Heat>0.3</ArmorRating_Heat>
      <Insulation_Cold>15</Insulation_Cold>
      <Insulation_Heat>15</Insulation_Heat>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>-2</MoveSpeed>
      <ToxicSensitivity>-0.5</ToxicSensitivity>
    </equippedStatOffsets>
    <costList>
      <ComponentIndustrial>10</ComponentIndustrial>
      <ComponentSpacer>3</ComponentSpacer>
      <Plasteel>45</Plasteel>
      <AeroTungstenCarbide>60</AeroTungstenCarbide>
    </costList>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
        <li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
      <tags>
        <li>AvaliApparel</li>
        <li>AvaliArmour</li>
        <li>AvaliSpecOps</li>
        <li>EVA</li>
      </tags>
      <wornGraphicPath>avali/Things/Apparel/SpecOpsArmor/SpecOpsArmor</wornGraphicPath>
      <layers>
        <li>Middle</li>
        <li>Shell</li>
      </layers>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
      <useDeflectMetalEffect>true</useDeflectMetalEffect>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>10</weight>
          <only>(0.9,0.9,0.9,1)</only>
        </li>
        <li>
          <weight>10</weight>
          <only>(0.33,0.33,0.33,1)</only>
        </li>
        <li>
          <weight>10</weight>
          <only>(0.65,0.65,0.65,1)</only>
        </li>
        <li>
          <weight>6</weight>
          <min>(0.3,0.3,0.3,1)</min>
          <max>(0.5,0.5,0.5,1)</max>
        </li>
      </options>
    </colorGenerator>
    <thingSetMakerTags>
      <li>RewardStandardHighFreq</li>
    </thingSetMakerTags>
  </ThingDef>

